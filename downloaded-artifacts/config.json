{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api(/.*)?$",
      "status": 404
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    },
    {
      "handle": "miss"
    },
    {
      "src": "^/api/(.+)(?:\\.(?:js|ts))$",
      "dest": "/api/$1",
      "check": true
    }
  ],
  "crons": []
}
